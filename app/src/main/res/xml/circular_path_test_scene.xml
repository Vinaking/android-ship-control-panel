<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:autoTransition="animateToEnd"
        motion:duration="2700">
        <KeyFrameSet>
            <KeyPosition
                motion:motionTarget="@+id/circle1"
                motion:framePosition="20"
                motion:keyPositionType="pathRelative"
                motion:percentX="0.15"
                motion:percentY="-0.18" />
            <KeyPosition
                motion:motionTarget="@+id/circle1"
                motion:framePosition="35"
                motion:keyPositionType="pathRelative"
                motion:percentX="0.3"
                motion:percentY="-0.25" />
            <KeyPosition
                motion:motionTarget="@+id/circle1"
                motion:framePosition="50"
                motion:keyPositionType="pathRelative"
                motion:percentX="0.5"
                motion:percentY="-0.29" />
            <KeyPosition
                motion:motionTarget="@+id/circle1"
                motion:framePosition="65"
                motion:keyPositionType="pathRelative"
                motion:percentX="0.7"
                motion:percentY="-0.25" />
            <KeyPosition
                motion:motionTarget="@+id/circle1"
                motion:framePosition="80"
                motion:keyPositionType="pathRelative"
                motion:percentX="0.85"
                motion:percentY="-0.18" />
            <KeyPosition
                motion:motionTarget="@+id/circle2"
                motion:framePosition="25"
                motion:keyPositionType="pathRelative"
                motion:percentX="0.3"
                motion:percentY="-0.16" />
            <KeyPosition
                motion:motionTarget="@+id/circle2"
                motion:framePosition="50"
                motion:keyPositionType="pathRelative"
                motion:percentX="0.65"
                motion:percentY="-0.17" />
            <KeyPosition
                motion:motionTarget="@+id/circle3"
                motion:framePosition="25"
                motion:keyPositionType="pathRelative"
                motion:percentX="0.29"
                motion:percentY="-0.16" />
            <KeyPosition
                motion:motionTarget="@+id/circle3"
                motion:framePosition="50"
                motion:keyPositionType="pathRelative"
                motion:percentX="0.65"
                motion:percentY="-0.18" />
            <KeyAttribute
                motion:motionTarget="@+id/circle1"
                motion:framePosition="90"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/circle2"
                motion:framePosition="90"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/circle3"
                motion:framePosition="90"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/circle1"
                motion:framePosition="100"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/circle2"
                motion:framePosition="100"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/circle3"
                motion:framePosition="100"
                android:alpha="0" />

        </KeyFrameSet>
    </Transition>
    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@+id/start"
        motion:autoTransition="jumpToStart">
        <KeyFrameSet >

        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint android:id="@+id/circle1">
            <Layout
                android:layout_width="30dp"
                android:layout_height="30dp"
                motion:layout_constraintTop_toTopOf="parent"
                motion:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="60dp"
                android:layout_marginTop="100dp"/>
            <PropertySet
                app:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/circle2">
            <Layout
                android:layout_height="30dp"
                android:layout_width="30dp"
                motion:layout_constraintTop_toTopOf="parent"
                motion:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="28dp"
                android:layout_marginTop="140dp"/>
            <PropertySet
                app:visibilityMode="ignore" />
        </Constraint>
        <Constraint android:id="@+id/circle3">
            <Layout
                android:layout_width="30dp"
                android:layout_height="30dp"
                motion:layout_constraintBottom_toBottomOf="parent"
                motion:layout_constraintTop_toTopOf="parent"
                motion:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="310dp"
                android:layout_marginStart="160dp"/>
            <PropertySet
                app:visibilityMode="ignore" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/circle1"
            android:layout_width="30dp"
            android:layout_height="30dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="60dp"
            android:layout_marginTop="100dp" />
        <Constraint
            android:id="@+id/circle2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="30dp"
            android:layout_height="30dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="155dp"
            android:layout_marginTop="312dp" />
        <Constraint
            android:id="@+id/circle3"
            android:layout_width="30dp"
            android:layout_height="30dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="25dp"
            android:layout_marginTop="150dp" />
        <Constraint
            android:id="@+id/circle4"
            android:layout_width="30dp"
            android:layout_height="30dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>
</MotionScene>